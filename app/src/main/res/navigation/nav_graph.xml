<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/navigation_info">

    <fragment
        android:id="@+id/navigation_info"
        android:name="com.example.finalexam.Fragments.info_Fragment"
        android:label="Info"
        tools:layout="@layout/info_fragment">
        <action
            android:id="@+id/action_info_Fragment_to_user_Fragment"
            app:destination="@id/navigation_data" />
    </fragment>
    <fragment
        android:id="@+id/navigation_data"
        tools:layout="@layout/user_fragment"
        android:name="com.example.finalexam.Fragments.user_Fragment"
        android:label="Data" />
</navigation>